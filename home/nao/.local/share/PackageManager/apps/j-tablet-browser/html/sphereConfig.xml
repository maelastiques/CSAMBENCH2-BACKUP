			<hsbRange hueFrom="0.13" hueTo="0.5" satFrom=".7" satTo=".9" briFrom=".7" briTo=".9"/>
			<hsbRange hueFrom="0.13" hueTo="0.5" satFrom=".7" satTo=".9" briFrom=".7" briTo=".9"/>
