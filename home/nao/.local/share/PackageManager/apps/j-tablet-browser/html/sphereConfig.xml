        <!-- color definition (hsbRange and entries are alone at this time, you can implements new type in java code if needed, @see jp.softbank.custombrowser.opengl.color.*) -->
        <!-- 0: cyan  unused-->
        <hsbRange hueFrom="0.45" hueTo="0.52" satFrom=".7" satTo=".8" briFrom=".9" briTo=".9"/>
        <!-- 1: pink reddish mix -->
        <entries>
            <!-- red to yellow-->
            <hsbRange hueFrom="0.0" hueTo="0.2" satFrom=".7" satTo=".9" briFrom=".7" briTo=".9"/>
            <!-- red to orange-->
            <hsbRange hueFrom="0.0" hueTo="0.15" satFrom=".7" satTo=".9" briFrom=".7" briTo=".9"/>
            <!-- purple to red-->
            <hsbRange hueFrom="0.8" hueTo="1.0" satFrom=".7" satTo=".9" briFrom=".7" briTo=".9"/>
            <hsbRange hueFrom="0.8" hueTo="1.0" satFrom=".7" satTo=".9" briFrom=".7" briTo=".9"/>
            <!-- darker purple to red-->
            <hsbRange hueFrom="0.8" hueTo="1.0" satFrom=".8" satTo="1" briFrom=".7" briTo=".9"/>
        </entries>
        <!-- 2 multicolored -->
        <hsbRange hueFrom="0.0" hueTo="1" satFrom=".9" satTo="1" briFrom=".7" briTo=".9"/>
        <!-- 3 blues -->
